{
  "manifest_version": 2,

  "name": "Zoom Closer Custom",
  "description": "This extension automatically closes the launched window from Zoom meetings.",
  "version": "1.61",

  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://zoom.us/postattendee",
        "https://zoom.us/j/*",
        "https://*.zoom.us/j/*",
        "https://zoom.us/s/*",
        "https://*.zoom.us/s/*",
        "https://teams.microsoft.com/*launcher/*",
        "https://teams.microsoft.com.mcas.ms/*launcher/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
